package com.iconmart.product.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table(name="categories")
public class Category {

	private Long id;
    private String name;
    private String description;
    private Double price; // Use Double for potential nulls or precision
    private Long categoryid;
    private Boolean active; // Use Boolean for true/false/null

	
}
